<div class="form-group col-lg-12 col-md-12 col-sm-12 text-center"><h4>SELECCIONAR SERVICIO A PRESENTAR</h4></div>
<div class="row">
    <div class="col-lg-12 col-md-12 col-sm-12">
        {{ form_label(form.servicio) }}
        {{ form_errors(form.servicio) }}
        <div class="col-lg-8 col-md-8 col-sm-12 pull-right">
            {{ form_widget(form.servicio) }}
        </div>
    </div>
</div>
<div class="form-group col-lg-12 col-md-12 col-sm-12 text-center"><h4>INGRESAR CUIT DEL COMITENTE/CLIENTE</h4></div>
<div class="row">
    <div class="col-lg-12 col-md-12 col-sm-12">
        {{ form_label(form.comitenteCuit) }}
        {{ form_errors(form.comitenteCuit) }}
        <div class="col-lg-8 col-md-8 col-sm-12 pull-right">
            {{ form_widget(form.comitenteCuit) }}
        </div>
    </div>
</div>
<br>
<div class="form-group col-lg-12 col-md-12">
    {{ form_row(form.save) }}
</div>