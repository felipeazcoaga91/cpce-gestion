    <style type="text/css">
        body {
            background:#e9ecef;
        }
        .content {
            background: inherit;
            box-shadow: none;
        }
        .footer {
            display: none;
        }
        .box{
            padding:60px 0px;
        }
        .box-part{
            background:#fff;
            border-radius: 15px 50px 30px;
            padding:60px 10px;
            margin:30px 0px;
        }
        .box-part:hover{
            -webkit-box-shadow: 0 4px 15px rgba(0,0,0,0.75);
            -moz-box-shadow: 0 4px 15px rgba(0,0,0,0.75);
            box-shadow: 0 4px 15px rgba(0,0,0,0.75);
            -webkit-transition: .5s;
            transition: .5s
        }
        .text{
            margin:20px 0px;
        }

        .fa{
            color:#4183D7;
        }
    </style>
{% endblock %}

