<fieldset>
    <legend>
        <h4>Datos Personales:</h4>
        <h5>(por favor verifique sus datos, de ser necesario) -> 
            <a class="btn btn-warning btn-sm" href="{{ path('front_dato_editar') }}"><b>Actualizar Datos</b></a>
        </h5>
    </legend>
    <div class="row">
        <div class="col-lg-12">
            <label class="control-label">Apellido y Nombres:</label>
            {{ afiliado.afiNombre }}
        </div>
        <div class="col-lg-4">
            <label class="control-label">DNI. Nº:</label>
            {{ afiliado.afiNroDoc }}
        </div>
        <div class="col-lg-4">
            <label class="control-label">TE:</label>
            {{ afiliado.afiTelefono1 }}
        </div>
        <div class="col-lg-4">
            <label class="control-label">CEL:</label>
            {{ afiliado.afiCelular }}
        </div>
        <div class="col-lg-12">
            <label class="control-label">E-mail:</label>
            {{ afiliado.afiMail }}
        </div>
        <div class="col-lg-12">
            <label class="control-label">Domicilio Particular:</label>
            {{ afiliado.afiDireccion }}
        </div>
        <div class="col-lg-4">
            <label class="control-label">Localidad:</label>
            {{ afiliado.afiLocalidad }}
        </div>
        <div class="col-lg-4">
            <label class="control-label">Provincia:</label>
            {{ afiliado.afiProvincia }}
        </div>
        <div class="col-lg-4">
            <label class="control-label">CP:</label>
            {{ afiliado.afiCodPos }}
        </div>
        <div class="col-lg-3">
            <label class="control-label">Título Profesional:</label>
            {{ afiliado.afiTitulo }}
        </div>
        <div class="col-lg-4">
            <label class="control-label">M.P. Nº:</label>
            {{ afiliado.afiMatricula }}
        </div>
    </div>
</fieldset>